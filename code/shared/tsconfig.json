{
	"extends": ["./.svelte-kit/tsconfig.json", "../../tsconfig.base.json"],
	"compilerOptions": {
		"outDir": "build",
		"composite": true,
		"noEmit": false
	},
	"include": ["src/**/*.js", "src/**/*.ts", "src/**/*.svelte"],
	"exclude": [
		"../node_modules/**",
		"../src/service-worker.js",
		"../src/service-worker/**/*.js",
		"../src/service-worker.ts",
		"../src/service-worker/**/*.ts",
		"../src/service-worker.d.ts",
		"../src/service-worker/**/*.d.ts"
	]
}
